{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
    "source": "/(.*)",
  "destination": "/index.html",
  "installCommand": "npm install --engine-strict=false --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}