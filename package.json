  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "postinstall@motia": "motia install",
    "dev@motia": "motia dev",
    "generate-types": "motia generate-types",
    "build@motia": "motia build",
    "clean": "rm -rf dist node_modules python_modules .motia .mermaid",
    "build:vercel": "npm install --legacy-peer-deps --force && vite build",
    "generate": "nuxt generate",
    "build@ts": "tsc && vite build",
    "drizzle@change" : "npx drizzle-kit push",
    "drizzle@generate" : "npx drizzle-kit generate",
    "drizzle@migrate" : "npx drizzle-kit migrate",
    "drizzle@pull": "npx drizzle-kit pull",
    "drizzle@run" : "npx tsx src/index.ts",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "worker:run": "rw-scripts worker-run",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
    "generate@redwood": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "dev@blitz": "blitz dev",
    "build@blitz": "blitz build",
    "start@blitz": "blitz start",
    "studio@blitz": "blitz prisma studio",
    "lint@blitz": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test@blitz": "vitest run --passWithNoTests",
    "test:watch@blitz": "vitest",
    "prepare@blitz": "husky install",
    "auto:dev": "bun dev || pnpm dev || yarn dev || npm run dev",
    "auto:install": "bun install || pnpm install || yarn install || npm install",
    "clean:full": "rimraf node_modules package-lock.json yarn.lock pnpm-lock.yaml bun.lockb && npm cache clean --force",
    "install:full": "npm run clean:full && npm install --legacy-peer-deps --force",
    "dev:marko": "marko-run",
    "build:marko": "marko-run build",
    "preview:marko": "marko-run preview",
    "start:marko": "marko-run start",
    "build:rollup": "rimraf dist && rollup -c -m --environment NODE_ENV:production",
    "dev:rollup": "rimraf dist && rollup -c -m -w",
    "dev:inspect:rollup": "INSPECT=1 npm run dev",
    "start:rollup": "npm run env NODE_ENV=production -- node dist",
    "build:all": "rimraf dist && npm run build:worker && npm run build:client",
    "build:client": "cross-env NODE_ENV=production vite build",
    "build:worker": "cross-env NODE_ENV=production vite build --ssr src/worker.js",
    "dev:all": "node --enable-source-maps src/dev-server.js",
    "dev:inspect": "node --inspect --enable-source-maps src/dev-server.js",
    "preview": "npx wrangler dev",
    "dev:vike": "vike dev",
    "build:vike": "vike build",
    "preview:vike": "vike build && vike preview",
    "deploy": "npm run build && wrangler pages deploy",
    "build:remix:client": "remix vite:build",
    "dev:remix": "node pre-start.cjs  && remix vite:dev",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app",
    "lint:fix": "npm run lint -- --fix && prettier app --write",
    "start:windows": "wrangler pages dev ./build/client",
    "start:unix": "bindings=$(./bindings.sh) && wrangler pages dev ./build/client $bindings",
    "start:remix": "node -e \"const { spawn } = require('child_process'); const isWindows = process.platform === 'win32'; const cmd = isWindows ? 'npm run start:windows' : 'npm run start:unix'; const child = spawn(cmd, { shell: true, stdio: 'inherit' }); child.on('exit', code => process.exit(code));\"",
    "dockerstart": "bindings=$(./bindings.sh) && wrangler pages dev ./build/client $bindings --ip 0.0.0.0 --port 5173 --no-show-interactive-dev-session",
    "dockerrun": "docker run -it -d --name bolt-ai-live -p 5173:5173 --env-file .env.local bolt-ai",
    "dockerbuild:prod": "docker build -t bolt-ai:production -t bolt-ai:latest --target bolt-ai-production .",
    "dockerbuild": "docker build -t bolt-ai:development -t bolt-ai:latest --target bolt-ai-development .",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview:remix": "pnpm run build && pnpm run start",
    "prepare": "husky",
    "clean:remix": "node scripts/clean.js",
    "clean:remix:dist": "rimraf dist",
    "build:remix:server": "remix vite:build --ssr",
    "build:remix:client:production": "remix vite:build --mode production",
    "build:remix": "npm run build:remix:client && npm run build:remix:server",
    "build:remix:client:productions": "remix vite:build --mode production",
    "build:remix:client:development": "remix vite:build",
    "build:worker:production": "cross-env NODE_ENV=production vite build --ssr src/worker.js",
    "build:client:production": "vite build --outDir=dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir=dist/server",
    "build:production": "npm run build:client:production && npm run build:server && npm run build:worker:production",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix:client": "eslint --fix .",
    "start:worker": "ts-node src/worker.ts",
    "start.watch": "nodemon src/worker.ts",
    "workflow": "ts-node src/client.ts"












