  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "postinstall@motia": "motia install",
    "dev@motia": "motia dev",
    "generate-types": "motia generate-types",
    "build@motia": "motia build",
    "clean": "rm -rf dist node_modules python_modules .motia .mermaid",
    "build:vercel": "npm install --legacy-peer-deps --force && vite build",
    "generate": "nuxt generate",
    "build@ts": "tsc && vite build",
    "drizzle@change" : "npx drizzle-kit push",
    "drizzle@generate" : "npx drizzle-kit generate",
    "drizzle@migrate" : "npx drizzle-kit migrate",
    "drizzle@pull": "npx drizzle-kit pull",
    "drizzle@run" : "npx tsx src/index.ts",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "worker:run": "rw-scripts worker-run",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
    "generate@redwood": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "dev@blitz": "blitz dev",
    "build@blitz": "blitz build",
    "start@blitz": "blitz start",
    "studio@blitz": "blitz prisma studio",
    "lint@blitz": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test@blitz": "vitest run --passWithNoTests",
    "test:watch@blitz": "vitest",
    "prepare@blitz": "husky install",
    "auto:dev": "bun dev || pnpm dev || yarn dev || npm run dev",
    "auto:install": "bun install || pnpm install || yarn install || npm install"












